{
    "name": "weareappa/locations",
    "description": "Geocoding base library",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0.2",
        "php-http/message": "^1.13",
        "laravel-doctrine/orm": "^1.7",
        "laravel-doctrine/fluent": "^1.2",
        "php-http/guzzle6-adapter": "^2.0",
        "geocoder-php/google-maps-provider": "^4.6",
        "ramsey/uuid": "^4.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^8.5 || ^9.5"
    },
    "autoload": {
        "psr-4": {
            "Digbang\\Locations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digbang\\Locations\\": "tests/"
        }
    },
    "license": "MIT"
}
